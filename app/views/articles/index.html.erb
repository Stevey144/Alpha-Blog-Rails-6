<div class="Articlecontainer">
<p style="color: green"><%= notice %></p>

<table class="tableTitle">
<tr><td>
<h1>Articles</h1>
</td> <td>
<%= link_to "New article", new_article_path , class: "myNewArticle" %>
</td></tr>
</table>

<div id="articles">
  <% @articles.each do |article| %>
    <%= render article %>
    <br>
    <br>
    <p>
      <%= link_to "Show this article", article, class: "indexshowArticle" %>
    </p>
  <% end %>
</div>

</div>



