<div class="Articlecontainer">
<p style="color: green"><%= notice %></p>

<h1>Articles</h1>

<div id="articles">
<table>
  <% @articles.each do |article| %>
    <%= render article %>
    <p>
      <%= link_to "Show this article", article, class: "indexshowArticle" %>
    </p>
  <% end %>
</div>

<%= link_to "New article", new_article_path %>
</div>



